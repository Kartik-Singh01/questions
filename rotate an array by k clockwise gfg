void reverse(int arr[], int s, int e){
    
        
        while(s<e){
            swap(arr[s], arr[e]);
            s++;
            e--;
        }
}

void rotate(int arr[], int n)
{
    int k =1;
        reverse(arr, 0, n-1);
        reverse(arr, 0, k-1);
        reverse(arr, k, n-1);
    
    
}
